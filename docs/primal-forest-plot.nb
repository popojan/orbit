  RowBox[{
   RowBox[{
    RowBox[{"comp1", "[", 
     RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
    RowBox[{"Join", "@@", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"k", "*", "p"}], "+", 
          RowBox[{"p", "^", "2"}]}], ",", 
         RowBox[{
          RowBox[{"k", "*", "p"}], "+", "1"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "0", ",", "n"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"p", ",", 
         RowBox[{"Max", "[", 
          RowBox[{"1", ",", 
           RowBox[{"Ceiling", "[", 
            RowBox[{
             RowBox[{"1", "/", "2"}], " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "k"}], "+", 
               RowBox[{"Sqrt", "[", 
                RowBox[{
                 RowBox[{"k", "^", "2"}], "+", 
                 RowBox[{"4", "*", "m"}]}], "]"}]}], ")"}]}], "]"}]}], "]"}], 
         ",", 
         RowBox[{"Floor", "[", 
          RowBox[{
           RowBox[{"1", "/", "2"}], " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "k"}], "+", 
             RowBox[{"Sqrt", "[", 
              RowBox[{
               RowBox[{"k", "^", "2"}], "+", 
               RowBox[{"4", "*", "n"}]}], "]"}]}], ")"}]}], "]"}]}], "}"}]}], 
      "]"}]}]}], "\n", 
   RowBox[{"(*", 
